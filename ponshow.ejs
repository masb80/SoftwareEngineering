<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
      <table class="table table-hover">
        <thead>
          <tr>
            <th>Name</th>
            <th>Party</th>
            <th>Age</th>
          </tr>
        </thead>
        <tbody>

            <tr >
              <% for(var i=0; i<candidates.length; i++) {%>
                <td><%= candidates[i].name %></td>
                  <td><%= candidates[i].party %></td>
                  <td><%= candidates[i].age %></td>
                </tr>
                <% } %>

        </tbody>
      </table>

  </body>
</html>
