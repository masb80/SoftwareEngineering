<!DOCTYPE html>
<html>
<head>
  <%include ../partials/head %>
</head>

<body>
  <div class="jumbotron" id="fortop" align="center">
    <h1 style="color:white;">Request Box</h1>
  </div>

  <div class="row">
    <div class="col-sm-6">
      <h2 align="center">Approvals Received </h2>
      <div class="row">
        <div class="col-sm-2">

        </div>
        <div class="col-sm-8">
          <table class="table table-hover">
            <thead>
              <tr>
                <th>From Candidate</th>
                <th>Message</th>
                <th>#</th>
              </tr>
            </thead>
            <tbody>
              <% for(var i=0; i<reqs.length; i++) {%>
              <tr>
                <td><%= reqs[i].from %></td>
                <td><%= reqs[i].content %></td>
                <td><a href="/party/request/resolve/<%= reqs[i]._id %>"><button type="submit" class="btn btn-primary">Resolve</button></a></td>

              </tr>
              <% }%>
            </tbody>
          </table>


        </div>

      </div>


    </div>


    <div class="col-sm-6">
      <h2 align="center"> Election Approval to EM </h2>

    </div>

  </div>


</body>
</html>
